{
  "name": "boostlol-browser",
  "displayName": "BoostLoL Browser",
  "version": "1.0.0",
  "description": "Browser Tools for BoostLOL",
  "author": "john@boostchicken.dev",
  "scripts": {
    "start": "next start",
    "dev": "run-p dev:*",
    "dev:plasmo": "plasmo dev",
    "dev:next": "next dev --port 1947",
    "build": "run-p build:*",
    "build:plasmo": "plasmo build",
    "build:next": "next build"
  },
  "dependencies": {
    "bootstrap": "^5.3.2",
    "next": "^13.5.4",
    "react": "18.2.0",
    "plasmo": "0.83.0", 
    "@plasmohq/storage": "1.8.0",
    "react-bootstrap": "^2.9.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.256",
    "@types/node": "20.5.9",
    "@types/react": "18.2.28",
    "@types/react-dom": "18.2.13",
    "prettier": "3.0.3",
    "typescript": "5.3.3"
    
  },
  "manifest": {
    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ],
    "permissions": [
      "activeTab",
      "storage",
      "tabs",
      "search"
    ],
    "omnibox": {
      "keyword": "omni"
    }
  }
}
