{
    "name": "boostchicken-lol-ext",
    "short_name": "lol",
    "description": "LOL Browser Integration",
    "homepage_url": "https://github.com/boostchicken/lol",
    "version": "0.0.2",
    "version_name": "0.0.2",
    "manifest_version": 3,
    "default_locale": "en",
    "minimum_chrome_version": "88",
    "omnibox": { "keyword": "bl" },
    "host_permissions": [
        "https://lol.boostchicken.dev/*"
    ],
    "icons": {
        "128": "assets/img/128x128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "16": "assets/img/16x16.png",
            "24": "assets/img/24x24.png",
            "32": "assets/img/32x32.png"
        },
        "default_title": "lol"
    }
}