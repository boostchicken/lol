{
    "name": "boostchicken-omnilol",
    "short_name": "omnilol",
    "description": "Show LOL commands in omnibox",
    "homepage_url": "https://github.com/boostchicken/lol",
    "version": "1.0.0",
    "version_name": "1.0.0",
    "manifest_version": 3,
    "default_locale": "en",
    "minimum_chrome_version": "88",
    "permissions": [
        "tabs",
        "search",
        "storage"
    ],
    "omnibox": { "keyword": "bl" },
    "host_permissions": [
        "<all_urls>"
    ],
    "icons": {
        "128": "assets/img/128x128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "16": "assets/img/16x16.png",
            "24": "assets/img/24x24.png",
            "32": "assets/img/32x32.png"
        },
        "default_title": "omnilol"
    }
}