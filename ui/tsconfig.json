{
  "extends": "@tsconfig/next/tsconfig.json",
  "compilerOptions": {
    "target": "es2022",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "incremental": true,
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "types": ["@cloudflare/workers-types"]
}
