{
  "name": "boostdev",
  "image": "mcr.microsoft.com/vscode/devcontainers/typescript-node:20",
  "features": {
    "ghcr.io/jungaretti/features/make:1": {},
    "ghcr.io/devcontainers/features/go:1": {
         "version": "latest"
    },
    "ghcr.io/lumenpink/devcontainer-features/bun:0": { },
    "ghcr.io/devcontainers-contrib/features/pnpm:2.0.3": {},
    "ghcr.io/devcontainers/features/common-utils:2": {
      "configureZshAsDefaultShell": true
    },
    "ghcr.io/devcontainers/features/docker-in-docker": {
      "installDockerBuildx": true
    },
    "ghcr.io/devcontainers-contrib/features/protoc:1.0.2": {
       "version":"v25.1"
    },
    "ghcr.io/devcontainers-contrib/features/apt-packages:1": {
      "packages": [
        "protoc-gen-go-grpc"
        ]
    },
    "ghcr.io/devcontainers-community/features/bazel:1.1": {},
    "ghcr.io/devcontainers/features/github-cli:1": {}
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "golang.go",
        "typescript.typescript-react-jsx",
        "protocolbuffer.vscode-proto3"
        
      ]
    }
  }
}
