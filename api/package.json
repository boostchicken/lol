{
  "name": "@boostchicken/lol-api",
  "version": "2.2.0",
  "description": "Genreated API for Boostchickens LOL clone",
  "main": "./src/index.ts",
  "type": "module",
  "types": "./src/index.d.ts",
  "exports": {
    ".": "./src/index.ts",
    "./clients/*.ts": "./src/clients/index.ts",
    "./models/*.ts": "./src/models/index.ts"
  },
  "scripts": {
    "build": "bun --bundle src/index.ts --outdir=dist  --sourcemap ",
    "start": "bun  --watch ",
    "typecheck": "tsc ./src/index.ts --declaration --emitDeclarationOnly",
    "generate": "kubb generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boostchicken/lol.git"
  },
  "author": "boostchicken",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/boostchicken/lol/issues"
  },
  "dependencies": {
    "@kubb/core": "1.14.6",
    "@kubb/swagger": "1.14.6",
    "@kubb/swagger-client": "1.14.6",
    "@kubb/swagger-swr": "1.14.6",
    "@kubb/swagger-ts": "1.14.6",
    "@redocly/cli": "^1.4.0",
    "axios": "^1.6.0",
    "swr": "^2.2.4",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@kubb/cli": "^1.14.6",
    "prettier": "3.0.3"
  }
}
